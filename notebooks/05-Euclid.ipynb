   "outputs": [],
   "source": [
    "def bad_append(new_item, a_list=[]):\n",
    "    a_list.append(new_item)\n",
    "    return a_list\n",
    "my_list = bad_append(1)\n",
    "print(bad_append(2))\n",
    "print(bad_append(3))"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
