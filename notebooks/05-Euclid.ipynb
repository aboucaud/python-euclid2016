   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "#### <font color='red'>NOT OK</font>"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "%%file my_specific_module.py\n",
    "try:   \n",
    "    raise ValueError(\"\"\"can not accept bla bla value\"\"\")\n",
    "except ValueError as e:   \n",
    "    print('Exception occurred')"
   ]
