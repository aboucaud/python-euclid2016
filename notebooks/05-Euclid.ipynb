    "collapsed": true
   },
   "outputs": [],
   "source": [
    "class MyClass(object):\n",
    "    def __init__(self, path_to_exec):\n",
    "        self.excutable = path_to_exec\n",
    "        \n",
    "class MyOtherClass(object):\n",
    "    def __init__(self):\n",
    "        self.speed_of_light_si = 3e8"
   ]
